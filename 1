
import javax.swing.JFrame;

import calc.Reader;

public class Body {
	public static void main(String args[]) {
	Reader r = new Reader("calc");
	r.setVisible(true);
	r.setDefaultCloseOperation(JFrame.EXIT_ON_CLOSE);
